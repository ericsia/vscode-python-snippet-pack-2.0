{
    "clear -dict": {
        "prefix": "clear -dict",
        "body": ".clear()",
        "description": "Removes all the elements from the dictionary"
    },
    "dict.clear=>": {
        "prefix": "dict-clear=>",
        "body": [
            "car = {",
            "\t'brand': 'Ford',",
            "\t'model': 'Mustang',",
            "\t'year': 1964",
            "}",
            "car.clear()",
            "print(car)"
        ],
        "description": "An example for using clear"
    },
